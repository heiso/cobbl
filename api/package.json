{
  "name": "@cobbl/api",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "watch": "tsc -w --pretty --preserveWatchOutput",
    "dev": "node --inspect=0.0.0.0:9229 --enable-source-maps --no-lazy -r dotenv-flow/config ./dist/src",
    "codegen": "graphql-codegen --config codegen.yml",
    "lint:ts": "tsc --noEmit",
    "prisma": "dotenv-flow -- prisma",
    "reset-database": "dotenv-flow -- prisma migrate reset -f --skip-generate"
  },
  "dependencies": {
    "@graphql-tools/mock": "^8.5.2",
    "@graphql-tools/schema": "^8.3.2",
    "@graphql-tools/utils": "^8.6.2",
    "@prisma/client": "^3.10.0",
    "faker": "^5.5.3",
    "graphql": "^16.3.0",
    "graphql-tag": "^2.12.6",
    "ioredis": "^4.28.5",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.1.0",
    "koa-trust-proxy": "^0.1.4",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@cobbl/graphql-codegen-typescript-operations-tester": "@cobbl/graphql-codegen-typescript-operations-tester",
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/schema-ast": "^2.4.1",
    "@graphql-codegen/typescript": "^2.4.7",
    "@graphql-codegen/typescript-operations": "^2.3.4",
    "@graphql-codegen/typescript-resolvers": "^2.5.4",
    "@types/dotenv-flow": "^3.2.0",
    "@types/faker": "^5.5.9",
    "@types/ioredis": "^4.28.8",
    "@types/ioredis-mock": "^5.6.0",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.6",
    "@types/koa-helmet": "^6.0.4",
    "@types/lodash": "^4.14.179",
    "dotenv-flow": "^3.2.0",
    "dotenv-flow-cli": "^1.0.0",
    "ioredis-mock": "^5.9.1",
    "node-mocks-http": "^1.11.0",
    "prisma": "^3.10.0"
  },
  "prisma": {
    "seed": "node ./dist/prisma/seed.js"
  }
}
